    <dubbo:reference id="clientCommandService" interface="com.atinbo.member.service.IClientCommandService" version="${dubbo.version}"/>
    <dubbo:reference id="clientQueryService" interface="com.atinbo.member.service.IClientQueryService" version="${dubbo.version}"/>
    <dubbo:reference id="userCommandService" interface="com.atinbo.member.service.IUserCommandService" version="${dubbo.version}"/>
