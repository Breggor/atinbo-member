    <dubbo:reference id="userAddressQueryService" interface="com.atinbo.member.service.IAddressService" version="${dubbo.version}"/>
    <dubbo:reference id="userDetailCommandService" interface="com.atinbo.member.service.IUserDetailService" version="${dubbo.version}"/>
    <dubbo:reference id="userDetailQueryService" interface="com.atinbo.member.service.IUserDetailQueryService" version="${dubbo.version}"/>
    <dubbo:reference id="userFamilyCommandService" interface="com.atinbo.member.service.IUserFamilyService" version="${dubbo.version}"/>
    <dubbo:reference id="userFamilyQueryService" interface="com.atinbo.member.service.IUserFamilyQueryService" version="${dubbo.version}"/>
    <dubbo:reference id="userInfoCommandService" interface="com.atinbo.member.service.IUserInfoCommandService" version="${dubbo.version}"/>
    <dubbo:reference id="userInfoQueryService" interface="com.atinbo.member.service.IUserInfoQueryService" version="${dubbo.version}"/>

    <!-- device -->
    <dubbo:reference id="braceletCommandService" interface="com.atinbo.member.service.bracelet.IBraceletCommandService" version="${dubbo.version}" check="false"/>
    <dubbo:reference id="braceletQueryService" interface="com.atinbo.member.service.bracelet.IBraceletQueryService" version="${dubbo.version}" check="false"/>
    <dubbo:reference id="braceletTipsCommandService" interface="com.atinbo.member.service.bracelet.IBraceletTipsCommandService" version="${dubbo.version}" check="false"/>
    <dubbo:reference id="braceletTipsQueryService" interface="com.atinbo.member.service.bracelet.IBraceletTipsQueryService" version="${dubbo.version}" check="false"/>

    <!-- integral -->
    <dubbo:reference id="integralConfigCommandService" interface="com.atinbo.member.service.integral.IIntegralConfigCommandService" version="${dubbo.version}"/>
    <dubbo:reference id="integralConfigQueryService" interface="com.atinbo.member.service.integral.IIntegralConfigQueryService" version="${dubbo.version}"/>
    <dubbo:reference id="integralRuleCommandSerice" interface="com.atinbo.member.service.integral.IIntegralRuleCommandService" version="${dubbo.version}"/>
    <dubbo:reference id="integralRuleQuerySerice" interface="com.atinbo.member.service.integral.IIntegralRuleQuerySerice" version="${dubbo.version}"/>
    <dubbo:reference id="integralRecordCommandService" interface="com.atinbo.member.service.integral.IIntegralRecordCommandService" version="${dubbo.version}"/>
    <dubbo:reference id="integralRecordQueryService" interface="com.atinbo.member.service.integral.IIntegralRecordQueryService" version="${dubbo.version}"/>
